<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yunxian.draggablelinearlistview.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DividerLayoutActivity"
            android:label="@string/title_one" />
        <activity
            android:name=".LinearListActivity"
            android:label="@string/title_two" />
        <activity
            android:name=".LinearListTwoActivity"
            android:label="@string/title_three" />
        <activity
            android:name=".DraggableListOneActivity"
            android:label="@string/title_four" />
        <activity
            android:name=".DraggableListTwoActivity"
            android:label="@string/title_five" />
        <activity
            android:name=".EmptyActivity"
            android:label="@string/title_six" />
    </application>

</manifest>
